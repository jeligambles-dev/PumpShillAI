# Solana
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
FEE_WALLET_PRIVATE_KEY=your_base58_private_key_here
SPENDING_WALLET_PRIVATE_KEY=your_spending_wallet_key_here

# Claude API
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Twitter/X
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_ACCESS_TOKEN=your_access_token
TWITTER_ACCESS_SECRET=your_access_token_secret

# Agent Config
POLL_INTERVAL_MS=60000
MIN_TREASURY_THRESHOLD_SOL=0.01
MAX_SPEND_PER_CAMPAIGN_PCT=10

# Telegram (optional — broadcasts campaigns to a channel)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHANNEL_ID=@your_channel_or_chat_id

# Twitter Ads (optional — for boosting tweets)
TWITTER_ADS_ACCOUNT_ID=your_twitter_ads_account_id
TWITTER_ADS_FUNDING_ID=your_twitter_ads_funding_instrument_id

# Crypto Ad Network (optional — A-Ads display ads)
A_ADS_API_TOKEN=your_a_ads_api_token

# Image Generation (optional — AI-generated images for tweets)
REPLICATE_API_TOKEN=your_replicate_api_token

# Farcaster Cross-Posting (optional — cross-post tweets to Warpcast)
ENABLE_FARCASTER=false
NEYNAR_API_KEY=your_neynar_api_key
FARCASTER_SIGNER_UUID=your_farcaster_signer_uuid

# Dashboard Auth (optional — password-protect admin panel)
ADMIN_PASSWORD=your_admin_password

# Shill Scanner (optional — find and reward organic Pumpfun shillers)
ENABLE_SHILL_SCANNER=true
SHILL_MIN_IMPRESSIONS=5000
SHILL_PAYMENT_SOL=0.1
SHILL_MAX_PAYMENTS_DAY=5

# Dashboard
DASHBOARD_PORT=3000

# Project Info (shown on landing page)
CONTRACT_ADDRESS=your_pumpfun_token_contract_address
TWITTER_HANDLE=PumpShillAI
TELEGRAM_CHANNEL=PumpShill_Chat
